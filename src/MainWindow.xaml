<Window
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:windows="clr-namespace:Matriks.ClientAPI.Setup.Core.Windows"
  xmlns:setup="clr-namespace:Matriks.ClientAPI.Setup"
  x:Class="Matriks.ClientAPI.Setup.MainWindow"
  Style="{StaticResource StyleWindow}"
  SnapsToDevicePixels="True"
  ResizeMode="CanMinimize"
  mc:Ignorable="d"
  windows:WindowFeatures.Statu="0"
  d:DataContext="{d:DesignInstance setup:MainWindow}"
  Title="CLIENT API MANAGER SETUP" Height="480" Width="640" Icon="Images/managerw.ico"
  FontFamily="/clientapimanager;component/Themes/Fonts/#Open Sans Condensed Light">


  <Grid>
    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="*" />
    </Grid.ColumnDefinitions>

    <Grid Grid.Column="0" Background="#FF1B191C">
      <Frame Name="LayoutRoot" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Template="{StaticResource ResourceKey=FrameKey}" />
    </Grid>
  </Grid>
</Window>