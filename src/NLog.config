<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
      autoReload="true"
      throwExceptions="false"
      internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log">

  <targets  async="true">
    <target xsi:type="File" name="clientapisetuplogfile" autoFlush="true" fileName="${basedir}/Logs/${date:format=yyyyMMdd}/clientapisetuplogfile_${date:format=yyyyMMdd}.log" archiveFileName="${basedir}/Logs/${date:format=yyyyMMdd}/clientapisetuplogfile.{####}.log" archiveAboveSize="100000000" />
  </targets>

  <rules>
    <logger name="ClientApiSetupLog" minlevel="Debug" writeTo="clientapisetuplogfile" />
  </rules>
</nlog>
